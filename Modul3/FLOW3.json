        "g": "a21ce3cb24965e6a",
        "group": "d08acf76a6f1a140",
        "page": "",
        "ui": "",
        "name": "Status grzejnika",
        "order": 2,
        "width": 0,
        "height": 0,
        "head": "",
        "format": "<style>\n    /* Kontener główny */\n    .status-container {\n        width: 100%; \n        height: 100%; \n        display: flex; \n        justify-content: space-between; \n        align-items: center; \n        padding: 0 25px; \n        box-sizing: border-box; \n        background-color: #ffffff !important; \n        border-radius: 8px; \n        border: 2px solid #bbb;\n        box-shadow: 0 4px 8px rgba(0,0,0,0.2);\n    }\n\n    /* Etykieta po lewej */\n    .status-label {\n        font-size: 14px; \n        font-weight: bold; \n        color: #555; \n        text-transform: uppercase;\n    }\n\n    /* Wartość po prawej */\n    .status-value {\n        font-size: 22px; \n        font-weight: 900;\n    }\n\n    /* Animacja pulsowania dla grzania */\n    @keyframes heating-pulse { \n        0% { opacity: 1; color: #ff4500; } \n        50% { opacity: 0.6; color: #d32f2f; } \n        100% { opacity: 1; color: #ff4500; } \n    }\n</style>\n\n<div class=\"status-container\">\n    <div class=\"status-label\">\n        STATUS GRZEJNIKA:\n    </div>\n\n    <div class=\"status-value\"\n         ng-style=\"{\n            'color': (msg.payload || '').indexOf('GRZEJE') !== -1 ? '#ff4500' : '#777',\n            'animation': (msg.payload || '').indexOf('GRZEJE') !== -1 ? 'heating-pulse 1.5s infinite' : 'none'\n         }\">\n        {{ (msg.payload || '').indexOf('GRZEJE') !== -1 ? 'GRZEJNIK WŁĄCZONY' : 'GRZEJNIK WYŁĄCZONY' }}\n    </div>\n</div>",
        "storeOutMessages": true,
        "passthru": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
        "x": 1340,
        "y": 820,
