            ]
        ]
    },
    {
        "id": "872eb4b4a3681b1b",
        "type": "function",
        "z": "a252394437443f81",
        "g": "d60daecf38ee1905",
        "name": "Sterowanie matą",
        "func": "//Pobierz stan z pamięciu lub ustaw domyślny\nlet target = context.get('target') || 25; //domyślnie 25 stopni\nlet mode = context.get('mode') || 0;\nlet temp = context.get('currentTemp') || 0;\nlet val = parseFloat(msg.payload);\n\n//Nadpisanie zmiennych jeśli przyjdzie poleceniee sterujące\nif (msg.topic === \"set_target\") context.set('target', target = val);\nif (msg.topic === \"mode\") context.set('mode', mode = val);\nif (msg.topic === \"temp_mata\") context.set('currentTemp', temp = val);\n\n//Porównanie wartości rzeczywistej i zadanej jeśli tryb manual\nif (mode === 1) {\n    let cmd = (temp < target) ? 1 : 0;\n    return { payload: cmd };\n}\nreturn null;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
